.game {
  height: 225rpx;
}

.hot-img {
  /* width: 100%; */
  height: 345rpx;
}

.title {
  position: relative;
  margin-bottom: 20rpx;
  text-indent: 30rpx;
  font-size: 16px;
  color: #3688FF;
}

.title::after {
  content: "";
  position: absolute;
  left: 0;
  top: 9rpx;
  width: 6rpx;
  height: 30rpx;
  background-color: #3688FF;
}

.more {
  position: relative;
  padding-right: 30rpx;
  font-size: 14px;
  color: #1a1a1a;
}

.more::after {
  content: "";
  position: absolute;
  top: 10rpx;
  right: 0;
  width: 16rpx;
  height: 16rpx;
  border-top: 1px solid #1a1a1a;
  border-right: 1px solid #1a1a1a;
  transform: rotate(45deg);
}

.hot-hd {
  align-items: center;
  margin-bottom: 30rpx;
}

.hot-bd {
  padding: 20rpx;
}

.hot-item {
  margin-bottom: 20rpx;
  font-size: 0;
}